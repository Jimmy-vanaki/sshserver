{
  "inbounds": [
    {
      "port": 9997,
      "protocol": "dokodemo-door",
      "settings": {
        "network": "tcp,udp",
        "timeout": 30,
        "followRedirect": true
      },
      "sniffing": {
        "enabled": false,
        "destOverride": ["http", "tls"],
        "metadataOnly": false
      }
    },
    {
      "port": 9998,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "sniffing": { "enabled": true, "destOverride": ["http", "tls"] },
      "settings": { "auth": "noauth", "udp": true }
    }
  ],
  "outbounds": [
    {
      "protocol": "socks",
      "settings": { "servers": [{ "address": "127.0.0.1", "port": 9999 }] }
    }
  ]
}
